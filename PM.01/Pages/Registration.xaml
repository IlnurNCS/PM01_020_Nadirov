<Page x:Class="PM._01.Pages.Registrarion"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:PM._01.Pages"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Registrarion">

    <Grid Background="White">
        <TextBox x:Name="Id" HorizontalAlignment="Left" Margin="120,116,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="228" Height="38" TextChanged="TextBox_TextChanged"/>
        <TextBox x:Name="Password" HorizontalAlignment="Left" Margin="120,187,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="228" Height="38" TextChanged="TextBox_TextChanged_1"/>
        <Button Content="Зарегестрироваться" HorizontalAlignment="Left" Margin="279,302,0,0" VerticalAlignment="Top" Height="51" Width="189"/>
        <Label x:Name="Labelid" Content="Id" HorizontalAlignment="Left" Margin="120,117,0,0" VerticalAlignment="Top" Height="37" Width="133" IsEnabled="False" IsHitTestVisible="False"/>
        <Label x:Name="Labelpass" Content="Password" HorizontalAlignment="Left" Margin="120,188,0,0" VerticalAlignment="Top" Height="37" Width="133" IsEnabled="False" IsHitTestVisible="False"/>

    </Grid>
</Page>
